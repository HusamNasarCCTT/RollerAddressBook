<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.husam.rolleraddressbook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:name=".app.AddressBookApp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.MainActivity"
            android:label="Address Book">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.AddNewContactActivity"
            android:label="@string/add_new_contact_activity" />
        <activity android:name=".Activity.ContactActivity"></activity>
    </application>


</manifest>